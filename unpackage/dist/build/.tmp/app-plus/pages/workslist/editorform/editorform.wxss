.switchbox {
	margin-left: 30px;
	-webkit-transform: scale(0.7);
	    -ms-transform: scale(0.7);
	        transform: scale(0.7);
	margin-top: -20rpx;
}
.collapse-box {
	margin-top: 10rpx;
